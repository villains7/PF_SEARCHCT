<% if project.thanked_by?(current_member) %>
  <%= link_to project_thanks_path(project),class: "btn_link", method: :delete,remote: true do %>
  Thanks<i class="fa fa-heart" aria-hidden="true" style="color: #ac2652;"></i>
  <%= project.thanks.count %>
  <% end %>
<% else %>
  <%= link_to project_thanks_path(project),class: "btn_link", method: :post,remote: true do%>
  Thanks<i class="far fa-heart" aria-hidden="true"></i>
  <%= project.thanks.count %>
  <% end %>
<% end %>