<%- if controller_name != 'sessions' %>
  <%= link_to "Log in", new_session_path(resource_name) ,class:"btn log_in_btn"%><br />
<% end %>

<%- if devise_mapping.registerable? && controller_name != 'registrations' %>
  <%= link_to "Sign up", new_registration_path(resource_name) ,class:"btn sign_up_btn"%><br />
<% end %>

